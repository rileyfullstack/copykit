{
  "name": "copykit",
  "displayName": "CopyKit",
  "description": "Easily copy files and folders with right-click actions",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:copykit.copyFileOrFolder",
    "onCommand:copykit.copyToClipboard"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "copykit.copyFileOrFolder",
        "title": "CopyKit: Copy to Temp File"
      },
      {
        "command": "copykit.copyToClipboard",
        "title": "CopyKit: Copy to Clipboard"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "copykit.copyFileOrFolder",
          "group": "copykit"
        },
        {
          "command": "copykit.copyToClipboard",
          "group": "copykit"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "eslint": "^8.56.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.3.3",
    "@vscode/test-electron": "^2.3.9"
  }
}